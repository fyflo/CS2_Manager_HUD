      if (!this.domCache.radarContainer) {
      this.domCache.radarContainer = document.getElementById("radar-players");
      }
      
      const radarContainer = this.domCache.radarContainer;
      if (!radarContainer) {
      return { x: 0, y: 0, level: "unknown" };
      }
      
      // РћРїС‚РёРјРёР·Р°С†РёСЏ: РїСЂРµРѕР±СЂР°Р·СѓРµРј position С‚РѕР»СЊРєРѕ РѕРґРёРЅ СЂР°Р·
      let posX, posY, posZ;
      
    if (typeof position === "string") {
      const posArray = position.split(", ").map(Number);
        posX = posArray[0];
        posY = posArray[1];
        posZ = posArray[2];
      } else if (Array.isArray(position)) {
        posX = position[0];
        posY = position[1];
        posZ = position[2];
      } else {
        posX = position.x;
        posY = position.y;
        posZ = position.z;
      }
      
      // РћРїС‚РёРјРёР·Р°С†РёСЏ: РєСЌС€РёСЂСѓРµРј РґРёР°РїР°Р·РѕРЅС‹ Z
    const lowerLevelRange = this.splits?.[0]?.zRange || {
      min: -Infinity,
      max: Infinity,
    };
      const upperLevelRange = this.zRange || { min: -Infinity, max: Infinity };
      
      // РћРїСЂРµРґРµР»СЏРµРј, РЅР° РєР°РєРѕРј СЌС‚Р°Р¶Рµ РЅР°С…РѕРґРёС‚СЃСЏ РѕР±СЉРµРєС‚
    const isLowerLevel =
      posZ >= lowerLevelRange.min && posZ <= lowerLevelRange.max;
    const isUpperLevel =
      posZ >= upperLevelRange.min && posZ <= upperLevelRange.max;
      
      // РћРїС‚РёРјРёР·Р°С†РёСЏ: РєСЌС€РёСЂСѓРµРј СЂР°Р·РјРµСЂС‹ РєРѕРЅС‚РµР№РЅРµСЂР°
      const containerWidth = radarContainer.offsetWidth;
      const containerHeight = radarContainer.offsetHeight;
      
      // РџСЂРµРѕР±СЂР°Р·СѓРµРј РєРѕРѕСЂРґРёРЅР°С‚С‹ РєР°СЂС‚С‹ РІ РїРёРєСЃРµР»Рё
    let x =
      ((posX + this.offset.x) / (1024 * this.resolution)) *
      containerWidth *
      1.02;
    let y =
      containerHeight -
      ((posY + this.offset.y) / (1024 * this.resolution)) *
        containerHeight *
        1.02;
      
      // РћРїС‚РёРјРёР·Р°С†РёСЏ: РїСЂРёРјРµРЅСЏРµРј СЃРјРµС‰РµРЅРёРµ С‚РѕР»СЊРєРѕ РµСЃР»Рё РЅСѓР¶РЅРѕ
      if (isLowerLevel && this.splits?.[0]) {
        const split = this.splits[0];
      x += ((split.offset.x * containerWidth) / 100) * 1.9;
      y -= ((split.offset.y * containerHeight) / 100) * 2.8;
      }
      
      return { 
        x, 
        y,
      level: isLowerLevel ? "lower" : isUpperLevel ? "upper" : "unknown",
      };
    };
  }

  function updateRadar(allplayers, map, data, observed, bomb, player, grenades) {
    // РРЅРёС†РёР°Р»РёР·Р°С†РёСЏ СЂР°РґР°СЂР°, РµСЃР»Рё РѕРЅ РµС‰Рµ РЅРµ РёРЅРёС†РёР°Р»РёР·РёСЂРѕРІР°РЅ
    if (!radarInitialized) {
      initializeRadar();
      radarInitialized = true;
    }
    
    // РџСЂРѕРІРµСЂРєР° РЅР° С„СЂРёР·С‚Р°Р№Рј РёР»Рё РІР°СЂРјР°Рї
  const isWarmup = data?.map?.phase === "warmup";
  const isFreezeTime = data?.phase_countdowns?.phase === "freezetime";
    
    // РћС‡РёС‰Р°РµРј СЂР°РґР°СЂ РїСЂРё РЅР°С‡Р°Р»Рµ С„СЂРёР·С‚Р°Р№РјР° РёР»Рё РІР°СЂРјР°РїР°
    if ((isWarmup || isFreezeTime) && !window.radar.lastPhase) {
      clearAllRadarElements();
    window.radar.lastPhase = isWarmup ? "warmup" : "freezetime";
    } else if (!isWarmup && !isFreezeTime && window.radar.lastPhase) {
      // РЎР±СЂР°СЃС‹РІР°РµРј С„Р»Р°Рі, РєРѕРіРґР° С„СЂРёР·С‚Р°Р№Рј РёР»Рё РІР°СЂРјР°Рї Р·Р°РєРѕРЅС‡РёР»РёСЃСЊ
      window.radar.lastPhase = null;
    }
    
    // РћРїС‚РёРјРёР·Р°С†РёСЏ: РїСЂРѕРІРµСЂСЏРµРј, РЅСѓР¶РЅРѕ Р»Рё РѕР±РЅРѕРІР»СЏС‚СЊ РєР°СЂС‚Сѓ
  if (
    map?.name &&
    (!window.radar.currentMap || window.radar.currentMap !== map.name)
  ) {
      // РћС‡РёС‰Р°РµРј РІСЃРµ РіСЂР°РЅР°С‚С‹ РїСЂРё СЃРјРµРЅРµ РєР°СЂС‚С‹
      clearAllGrenades();
      
      window.radar.currentMap = map.name;
      window.radar.loadMapData(map.name);
    }
    
    // РћРїС‚РёРјРёР·Р°С†РёСЏ: СѓРІРµР»РёС‡РёРІР°РµРј СЃС‡РµС‚С‡РёРє РєР°РґСЂРѕРІ
    window.radar.frameCounter = (window.radar.frameCounter + 1) % 60;
    
    // РћР±РЅРѕРІР»РµРЅРёРµ Р±РѕРјР±С‹ С‚РѕР»СЊРєРѕ РµСЃР»Рё РµСЃС‚СЊ РґР°РЅРЅС‹Рµ Рѕ Р±РѕРјР±Рµ
    if (window.radar && bomb) {
    const bombElement = document.querySelector("#bomb-container");
      updateBombState(bomb, bombElement);
    } else if (window.radar && window.radar.frameCounter % 10 === 0) {
      // РџСЂРѕРІРµСЂСЏРµРј СЂРµР¶Рµ РґР»СЏ РѕРїС‚РёРјРёР·Р°С†РёРё
    const bombElement = document.querySelector("#bomb-container");
      if (bombElement) {
      bombElement.style.display = "none";
      }
    }
    
    // РћР±РЅРѕРІР»РµРЅРёРµ РёРіСЂРѕРєРѕРІ
    if (allplayers && window.radar) {
      // РћР±РЅРѕРІР»СЏРµРј РіР»РѕР±Р°Р»СЊРЅСѓСЋ РїРµСЂРµРјРµРЅРЅСѓСЋ Р°РєС‚РёРІРЅРѕРіРѕ РёРіСЂРѕРєР°
      window.activePlayerSteamId = data?.player?.steamid || observed?.steamid;
      
      // РћРїС‚РёРјРёР·Р°С†РёСЏ: РєСЌС€РёСЂСѓРµРј DOM СЌР»РµРјРµРЅС‚С‹
      if (!window.radar.domCache.radarContainer) {
      window.radar.domCache.radarContainer =
        document.getElementById("radar-players");
      }
      const radarContainer = window.radar.domCache.radarContainer;
      
      // РћРїС‚РёРјРёР·Р°С†РёСЏ: РѕР±РЅРѕРІР»СЏРµРј РёРіСЂРѕРєРѕРІ С‚РѕР»СЊРєРѕ РєР°Р¶РґС‹Р№ РІС‚РѕСЂРѕР№ РєР°РґСЂ
      if (window.radar.frameCounter % 2 === 0) {
        // РЎРѕР·РґР°РµРј Set СЃ ID С‚РµРєСѓС‰РёС… РёРіСЂРѕРєРѕРІ РґР»СЏ Р±С‹СЃС‚СЂРѕРіРѕ РїРѕРёСЃРєР°
        const currentPlayerIds = new Set();
        
        // РћР±РЅРѕРІР»СЏРµРј РІСЃРµС… РёРіСЂРѕРєРѕРІ
        Object.entries(allplayers).forEach(([steamid, player]) => {
          if (!steamid) return;
          
          currentPlayerIds.add(`player-${steamid}`);
          
          // Р”РѕР±Р°РІР»СЏРµРј steamid РІ РѕР±СЉРµРєС‚ РёРіСЂРѕРєР°, РµСЃР»Рё РµРіРѕ РЅРµС‚
          if (!player.steamid) {
            player.steamid = steamid;
          }
          
          let playerElement = document.getElementById(`player-${steamid}`);
          
          // Р•СЃР»Рё СЌР»РµРјРµРЅС‚ РЅРµ СЃСѓС‰РµСЃС‚РІСѓРµС‚, СЃРѕР·РґР°РµРј РµРіРѕ
          if (!playerElement) {
            playerElement = createPlayerElement(player, steamid);
            if (radarContainer && playerElement) {
              radarContainer.appendChild(playerElement);
            }
          }
          
          // РћР±РЅРѕРІР»СЏРµРј С†РІРµС‚ Рё СЃР»РѕС‚ РёРіСЂРѕРєР°
          if (playerElement) {
            // РћРїСЂРµРґРµР»СЏРµРј, РЅРµСЃРµС‚ Р»Рё РёРіСЂРѕРє Р±РѕРјР±Сѓ
            const hasBomb = steamid === bomb?.player;
            
            // РћР±РЅРѕРІР»СЏРµРј С†РІРµС‚ РІ Р·Р°РІРёСЃРёРјРѕСЃС‚Рё РѕС‚ РєРѕРјР°РЅРґС‹ Рё РЅР°Р»РёС‡РёСЏ Р±РѕРјР±С‹
            if (hasBomb) {
              playerElement.style.backgroundColor = PLAYER_ORANGE;
            } else {
            playerElement.style.backgroundColor =
              player.team.toLowerCase() === "ct" ? COLOR_NEW_CT : COLOR_NEW_T;
            }
            
            // РћР±СЂР°Р±РѕС‚РєР° СЃР»РѕС‚РѕРІ
            let slotIdentifier;
          if (typeof player.observer_slot === "number") {
            slotIdentifier =
              player.observer_slot === 9 ? 0 : player.observer_slot + 1;
            } else {
            slotIdentifier = "N/A";
            }
            
            // РћР±РЅРѕРІР»СЏРµРј РЅРѕРјРµСЂ СЃР»РѕС‚Р° РІ СЌР»РµРјРµРЅС‚Рµ
          const slotElement = playerElement.querySelector(".player-slot");
            if (slotElement) {
              slotElement.textContent = slotIdentifier;
            }
          playerElement.setAttribute("data-slot", slotIdentifier);
            
            // Р’С‹Р±СЂР°РЅРЅРѕРјСѓ РёРіСЂРѕРєСѓ РґРѕР±Р°РІР»СЏРµРј РєР»Р°СЃСЃ selected
          playerElement.classList.toggle(
            "active_player_map",
            steamid === window.activePlayerSteamId
          );
          }
          
          // РџСЂРѕРІРµСЂСЏРµРј, РёР·РјРµРЅРёР»Р°СЃСЊ Р»Рё РїРѕР·РёС†РёСЏ РёРіСЂРѕРєР°
        const position = player.position
          ? player.position.split(", ").map(Number)
          : null;
          
        if (
          position &&
          (!player.lastPosition ||
              Math.abs(position[0] - player.lastPosition.x) > 0.5 || 
              Math.abs(position[1] - player.lastPosition.y) > 0.5 || 
            Math.abs(position[2] - player.lastPosition.z) > 0.5)
        ) {
            // РћР±РЅРѕРІР»СЏРµРј РїРѕР·РёС†РёСЋ РёРіСЂРѕРєР°
            window.radar.updatePlayer(player, {
              x: position[0],
              y: position[1],
            z: position[2],
            });
            
            // РЎРѕС…СЂР°РЅСЏРµРј РїРѕСЃР»РµРґРЅСЋСЋ РїРѕР·РёС†РёСЋ
          player.lastPosition = {
            x: position[0],
            y: position[1],
            z: position[2],
          };
          }
        });
        
        // РћРїС‚РёРјРёР·Р°С†РёСЏ: СѓРґР°Р»СЏРµРј СЌР»РµРјРµРЅС‚С‹ РёРіСЂРѕРєРѕРІ, РєРѕС‚РѕСЂС‹С… РЅРµС‚ РІ С‚РµРєСѓС‰РµРј СЃРїРёСЃРєРµ
        // Р”РµР»Р°РµРј СЌС‚Рѕ СЂРµР¶Рµ РґР»СЏ РѕРїС‚РёРјРёР·Р°С†РёРё
        if (window.radar.frameCounter % 10 === 0 && radarContainer) {
        const currentPlayerElements = Array.from(
          radarContainer.querySelectorAll('[id^="player-"]')
        );
        currentPlayerElements.forEach((element) => {
            if (!currentPlayerIds.has(element.id)) {
              element.remove();
            }
          });
        }
      }
    }
    
    // РћР±РЅРѕРІР»РµРЅРёРµ РіСЂР°РЅР°С‚ СЃ РѕРіСЂР°РЅРёС‡РµРЅРёРµРј С‡Р°СЃС‚РѕС‚С‹
    if (grenades && window.radar && window.radar.frameCounter % 1 === 0) {
      updateGrenades(grenades, allplayers);
    }
  }

// Р”РѕР±Р°РІР»СЏРµРј РЅРѕРІСѓСЋ С„СѓРЅРєС†РёСЋ РґР»СЏ РѕС‡РёСЃС‚РєРё РІСЃРµС… СЌР»РµРјРµРЅС‚РѕРІ СЂР°РґР°СЂР°
function clearAllRadarElements() {
  // РћС‡РёС‰Р°РµРј РІСЃРµ СЌР»РµРјРµРЅС‚С‹ РёРіСЂРѕРєРѕРІ
  document.querySelectorAll('[id^="player-"]').forEach((el) => el.remove());
  
  // РћС‡РёС‰Р°РµРј РІСЃРµ СЌР»РµРјРµРЅС‚С‹ РіСЂР°РЅР°С‚
  document
    .querySelectorAll(
      ".grenade, .inferno-flame, .inferno-container, .flame-point"
    )
    .forEach((el) => el.remove());
  
  // РћС‡РёС‰Р°РµРј Р±РѕРјР±Сѓ
  document.querySelectorAll("#bomb-container").forEach((el) => el.remove());
  
  // РћС‡РёС‰Р°РµРј РєРѕРЅС‚РµР№РЅРµСЂС‹ РіСЂР°РЅР°С‚
  const grenadeContainers = [
    "#smokes",
    "#flashbangs",
    "#hegrenades",
    "#infernos",
  ];
  grenadeContainers.forEach((selector) => {
    const container = document.querySelector(selector);
    if (container) container.innerHTML = "";
  });
}
  
function createPlayerElement(player, steamid) {
  if (!steamid || !player) {
      //console.error('РќРµРґРѕСЃС‚Р°С‚РѕС‡РЅРѕ РґР°РЅРЅС‹С… РґР»СЏ СЃРѕР·РґР°РЅРёСЏ СЌР»РµРјРµРЅС‚Р° РёРіСЂРѕРєР°:', { steamid, player });
      return null;
  }

  // РЎРѕР·РґР°РµРј РѕСЃРЅРѕРІРЅРѕР№ СЌР»РµРјРµРЅС‚ РёРіСЂРѕРєР°
  const playerElement = document.createElement("div");
  playerElement.id = `player-${steamid}`;
  playerElement.className = `player ${player.team.toLowerCase()}`;
  playerElement.setAttribute("data-steamid", steamid);

  // РЈСЃС‚Р°РЅР°РІР»РёРІР°РµРј С†РІРµС‚ РІ Р·Р°РІРёСЃРёРјРѕСЃС‚Рё РѕС‚ РєРѕРјР°РЅРґС‹
  playerElement.style.backgroundColor =
    player.team === "CT" ? COLOR_NEW_CT : COLOR_NEW_T;

  // РћР±РЅРѕРІР»СЏРµРј СЃС‚РёР»Рё РґР»СЏ РјРµСЂС‚РІС‹С… РёРіСЂРѕРєРѕРІ
  if (player.state.health <= 0) {
    playerElement.classList.add("dead_map");
  } else {
    playerElement.classList.remove("dead_map");
    playerElement.style.transition = "transform 0.15s ease-out";
  }

  // РџСЂРµРѕР±СЂР°Р·СѓРµРј observer_slot РІ РЅСѓР¶РЅС‹Р№ РёРґРµРЅС‚РёС„РёРєР°С‚РѕСЂ
  let slotIdentifier;
  if (player.observer_slot === 9) {
      slotIdentifier = 0; // 9 -> 0
  } else if (player.observer_slot >= 0 && player.observer_slot < 9) {
      slotIdentifier = player.observer_slot + 1; // 0 -> 1, 1 -> 2, ..., 8 -> 9
  } else if (player.observer_slot >= 10) {
      slotIdentifier = player.observer_slot; // 10 -> 10, 11 -> 11, Рё С‚.Рґ.
  } else {
    slotIdentifier = "N/A"; // Р•СЃР»Рё СЃР»РѕС‚ РЅРµРёР·РІРµСЃС‚РµРЅ
  }

 // Р”РѕР±Р°РІР»СЏРµРј РїСЂРѕРІРµСЂРєСѓ РЅР° РЅР°Р»РёС‡РёРµ Р±РѕРјР±С‹
  const hasBomb = Object.values(player.weapons || {}).some(
    (weapon) => weapon.type === "C4"
  );
 if (hasBomb) {
     playerElement.style.backgroundColor = PLAYER_ORANGE;
 }

  // РЈСЃС‚Р°РЅР°РІР»РёРІР°РµРј data-slot
  playerElement.setAttribute("data-slot", slotIdentifier);

  // РЎРѕР·РґР°РµРј СЌР»РµРјРµРЅС‚ РґР»СЏ РЅРѕРјРµСЂР° СЃР»РѕС‚Р°
  const slotElement = document.createElement("div");
  slotElement.className = "player-slot";
  slotElement.textContent = slotIdentifier;
  playerElement.appendChild(slotElement);

  // РЎРѕР·РґР°РµРј РґРѕС‡РµСЂРЅРёРµ СЌР»РµРјРµРЅС‚С‹ (РјР°СЂРєРµСЂ Рё СЌС„С„РµРєС‚ РѕРіРЅСЏ)
  const elements = {
      marker: createMarkerContainer(),
    fire: createFireContainer(),
  };

  // Р”РѕР±Р°РІР»СЏРµРј РґРѕС‡РµСЂРЅРёРµ СЌР»РµРјРµРЅС‚С‹ РІ РѕСЃРЅРѕРІРЅРѕР№ СЌР»РµРјРµРЅС‚
  Object.values(elements).forEach((el) => playerElement.appendChild(el));

  // Р’РѕР·РІСЂР°С‰Р°РµРј СЃРѕР·РґР°РЅРЅС‹Р№ СЌР»РµРјРµРЅС‚
  return playerElement;
}
  
  function createMarkerContainer() {
  const container = document.createElement("div");
  container.className = "player-marker";

  const triangle = document.createElement("div");
  triangle.className = "player-triangle";
    
    container.appendChild(triangle);
    /*container.appendChild(fire);*/
    return container;
  }

  function formatObserverSlot(slot) {
    // РЈРґР°Р»СЏРµРј console.log, РєРѕС‚РѕСЂС‹Р№ РјРѕР¶РµС‚ Р·Р°РјРµРґР»СЏС‚СЊ СЂР°Р±РѕС‚Сѓ
    const slotNumber = parseInt(slot);
  return slotNumber === 9 ? "0" : (slotNumber + 1).toString();
  }
  
  function createSlotElement(player) {
    // РЈРґР°Р»СЏРµРј console.log, РєРѕС‚РѕСЂС‹Р№ РјРѕР¶РµС‚ Р·Р°РјРµРґР»СЏС‚СЊ СЂР°Р±РѕС‚Сѓ
  const element = document.createElement("div");
  element.className = "player-slot";
    
    // РџСЂРѕРІРµСЂСЏРµРј, С‡С‚Рѕ observer_slot СЃСѓС‰РµСЃС‚РІСѓРµС‚ Рё СЏРІР»СЏРµС‚СЃСЏ С‡РёСЃР»РѕРј
    if (player.observer_slot !== undefined) {
      element.textContent = formatObserverSlot(player.observer_slot);
    } else {
    element.textContent = "N/A";
    }
    
    return element;
  }
  
  function createFireContainer() {
  const container = document.createElement("div");
  container.className = "background-fire";
    
  const bg = document.createElement("div");
  bg.className = "bg";
    
    container.appendChild(bg);
  container.style.display = "none";
    // Р”РѕР±Р°РІР»СЏРµРј РїРѕР·РёС†РёРѕРЅРёСЂРѕРІР°РЅРёРµ
  container.style.position = "absolute";
  container.style.transformOrigin = "center center";
    // РЎРјРµС‰Р°РµРј РєРѕРЅС‚РµР№РЅРµСЂ Р·Р° РїСЂРµРґРµР»С‹ РјР°СЂРєРµСЂР° РёРіСЂРѕРєР°
  container.style.left = "50%";
  container.style.top = "50%";
  container.style.transform = "translate(-50%, -50%)";
    
    return container;
}
  
function updatePlayerElement(element, player) {
  if (!element || !player) {
    //console.error('РќРµРґРѕСЃС‚Р°С‚РѕС‡РЅРѕ РґР°РЅРЅС‹С… РґР»СЏ РѕР±РЅРѕРІР»РµРЅРёСЏ СЌР»РµРјРµРЅС‚Р° РёРіСЂРѕРєР°:', { element, player });
    return;
  }

  // РћРїСЂРµРґРµР»СЏРµРј, РЅРµСЃРµС‚ Р»Рё РёРіСЂРѕРє Р±РѕРјР±Сѓ
  const hasBomb = player.steamid === bomb?.player;

  // РћР±РЅРѕРІР»СЏРµРј С†РІРµС‚ РІ Р·Р°РІРёСЃРёРјРѕСЃС‚Рё РѕС‚ РєРѕРјР°РЅРґС‹ Рё РЅР°Р»РёС‡РёСЏ Р±РѕРјР±С‹
  if (hasBomb) {
    element.style.backgroundColor = PLAYER_ORANGE;
  } else {
    element.style.backgroundColor =
      player.team.toLowerCase() === "ct" ? COLOR_NEW_CT : COLOR_NEW_T;
  }

  // РћР±СЂР°Р±РѕС‚РєР° СЃР»РѕС‚РѕРІ
  let slotIdentifier;
  if (typeof player.observer_slot === "number") {
    if (player.observer_slot === 9) {
      slotIdentifier = 0;
    } else if (player.observer_slot >= 0 && player.observer_slot < 9) {
      slotIdentifier = player.observer_slot + 1;
    } else {
      slotIdentifier = player.observer_slot;
    }
  } else {
    slotIdentifier = "N/A";
  }

  // РћР±РЅРѕРІР»СЏРµРј РЅРѕРјРµСЂ СЃР»РѕС‚Р° РІ СЌР»РµРјРµРЅС‚Рµ
  const slotElement = element.querySelector(".player-slot");
  if (slotElement) {
    slotElement.textContent = slotIdentifier;
  }

  // РћР±РЅРѕРІР»СЏРµРј data-slot Р°С‚СЂРёР±СѓС‚
  element.setAttribute("data-slot", slotIdentifier);
}
  
    function updateBombState(bomb, bombElement) {
      // Р•СЃР»Рё РЅРµС‚ РґР°РЅРЅС‹С… Рѕ Р±РѕРјР±Рµ РёР»Рё РЅРµС‚ РїРѕР·РёС†РёРё, СЃРєСЂС‹РІР°РµРј СЌР»РµРјРµРЅС‚ Р±РѕРјР±С‹
      if (!bomb || !bomb.position) {
          //console.warn('No bomb data or position');
          if (bombElement) {
      bombElement.style.display = "none";
          }
          return;
