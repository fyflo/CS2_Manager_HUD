    // const socketProtocol = isHttps ? "wss:" : "ws:";
    // const socketPort = isHttps ? "2627" : "2626";
    
    const socketProtocol = "ws:";
    const socketPort = "2626";
    
    const socketUrl = `${socketProtocol}//${window.location.hostname}:${socketPort}`;
    //console.log("РџРѕРґРєР»СЋС‡РµРЅРёРµ Рє WebSocket:", socketUrl);
    
    const socket = io(socketUrl, {
      transports: ["websocket"],
      reconnection: true,
      reconnectionAttempts: 5,
      reconnectionDelay: 1000,
      // secure: isHttps,
    });
    
    socket.on("connect", function() {
      //console.log("РЎРѕРµРґРёРЅРµРЅРёРµ СѓСЃС‚Р°РЅРѕРІР»РµРЅРѕ С‡РµСЂРµР· HTTP");
      socket.emit("ready");
    });
    
    socket.on("connect_error", function(error) {
      //console.error("РћС€РёР±РєР° РїРѕРґРєР»СЋС‡РµРЅРёСЏ:", error);
      // РЈРґР°Р»СЏРµРј РїРѕРїС‹С‚РєСѓ РїРµСЂРµРєР»СЋС‡РµРЅРёСЏ РЅР° HTTP
      // if (isHttps) {
      //   console.log("РќРµ СѓРґР°Р»РѕСЃСЊ РїРѕРґРєР»СЋС‡РёС‚СЊСЃСЏ РїРѕ HTTPS, РїСЂРѕР±СѓРµРј HTTP...");
      //   const httpSocketUrl = `ws://${window.location.hostname}:2626`;
      //   socket.io.uri = httpSocketUrl;
      //   socket.connect();
      // }
    });
    
    // ... РѕСЃС‚Р°Р»СЊРЅС‹Рµ РѕР±СЂР°Р±РѕС‚С‡РёРєРё СЃРѕР±С‹С‚РёР№ ...
    
    return socket;
  } catch (error) {
    //console.error("РћС€РёР±РєР° РїСЂРё СЃРѕР·РґР°РЅРёРё WebSocket:", error);
    return null;
  }
}
