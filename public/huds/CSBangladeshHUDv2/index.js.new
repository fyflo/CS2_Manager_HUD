// РћР±СЉСЏРІР»СЏРµРј РєРѕРЅСЃС‚Р°РЅС‚Сѓ РґР»СЏ РёРјРµРЅРё HUD
const HUD_NAME = "default";

let currentPhaseState = {
  phase: null,
  round: null,
};

// Р”РѕР±Р°РІРёС‚СЊ РІ РіР»РѕР±Р°Р»СЊРЅСѓСЋ РѕР±Р»Р°СЃС‚СЊ
let lastFrameTime = 0;
const targetFPS = 30;
const frameDuration = 1000 / targetFPS;
let pendingDataUpdate = null;

// Р—Р°РјРµРЅРёС‚СЊ С‚РµРєСѓС‰СѓСЋ С„СѓРЅРєС†РёСЋ updateHUD РЅР° СЌС‚Сѓ
function updateHUD(data) {
  // РЎРѕС…СЂР°РЅСЏРµРј РґР°РЅРЅС‹Рµ РґР»СЏ СЃР»РµРґСѓСЋС‰РµРіРѕ РѕР±РЅРѕРІР»РµРЅРёСЏ
  pendingDataUpdate = data;

  // Р•СЃР»Рё РѕР±РЅРѕРІР»РµРЅРёРµ РЅРµ Р·Р°РїР»Р°РЅРёСЂРѕРІР°РЅРѕ, РїР»Р°РЅРёСЂСѓРµРј РµРіРѕ
  if (!window.pendingUpdate) {
    window.pendingUpdate = true;
    requestAnimationFrame(frameRateControlledUpdate);
  }

  // FPS counter - РѕСЃС‚Р°РІР»СЏРµРј РєР°Рє РµСЃС‚СЊ
  if (!window._fpsData) {
    window._fpsData = {
      frames: 0,
      lastSecond: performance.now(),
      lastUpdate: performance.now(),
      fps: 0,
      fpsHistory: [],
      updateInterval: 1000,
    };
  }

  const now = performance.now();
  window._fpsData.frames++;

  if (now - window._fpsData.lastSecond >= window._fpsData.updateInterval) {
    window._fpsData.fps = Math.round(
      (window._fpsData.frames * 1000) / (now - window._fpsData.lastSecond)
    );
    window._fpsData.fpsHistory.push(window._fpsData.fps);

    if (window._fpsData.fpsHistory.length > 10) {
      window._fpsData.fpsHistory.shift();
    }

    const avgFps = Math.round(
      window._fpsData.fpsHistory.reduce((a, b) => a + b, 0) /
        window._fpsData.fpsHistory.length
    );
    const minFps = Math.min(...window._fpsData.fpsHistory);
    const maxFps = Math.max(...window._fpsData.fpsHistory);

    //console.log(
    //  `HUD FPS: ${window._fpsData.fps} (Avg: ${avgFps}, Min: ${minFps}, Max: ${maxFps}) [Target: ${targetFPS}]`
    //);

    window._fpsData.frames = 0;
    window._fpsData.lastSecond = now;
  }
}

// Р¤СѓРЅРєС†РёСЏ РґР»СЏ РєРѕРЅС‚СЂРѕР»СЏ С‡Р°СЃС‚РѕС‚С‹ РєР°РґСЂРѕРІ
function frameRateControlledUpdate(timestamp) {
  window.pendingUpdate = false;

  if (!pendingDataUpdate) return;

  const elapsed = timestamp - lastFrameTime;

  // Р•СЃР»Рё РїСЂРѕС€Р»Рѕ РґРѕСЃС‚Р°С‚РѕС‡РЅРѕ РІСЂРµРјРµРЅРё СЃ РїРѕСЃР»РµРґРЅРµРіРѕ РєР°РґСЂР° РёР»Рё СЌС‚Рѕ РїРµСЂРІС‹Р№ РєР°РґСЂ
  if (elapsed >= frameDuration || lastFrameTime === 0) {
    // РћР±РЅРѕРІР»СЏРµРј РІСЂРµРјСЏ РїРѕСЃР»РµРґРЅРµРіРѕ РєР°РґСЂР°
    lastFrameTime = timestamp - (elapsed % frameDuration);

    // Р’С‹РїРѕР»РЅСЏРµРј С„Р°РєС‚РёС‡РµСЃРєРѕРµ РѕР±РЅРѕРІР»РµРЅРёРµ
    actuallyUpdateHUD(pendingDataUpdate);

    // РћС‚РјРµС‡Р°РµРј, С‡С‚Рѕ РґР°РЅРЅС‹Рµ РѕР±СЂР°Р±РѕС‚Р°РЅС‹
    pendingDataUpdate = null;
  } else {
    // Р•СЃР»Рё РЅРµ РїСЂРѕС€Р»Рѕ РґРѕСЃС‚Р°С‚РѕС‡РЅРѕ РІСЂРµРјРµРЅРё, РїР»Р°РЅРёСЂСѓРµРј СЃР»РµРґСѓСЋС‰СѓСЋ РїСЂРѕРІРµСЂРєСѓ
    window.pendingUpdate = true;
    requestAnimationFrame(frameRateControlledUpdate);
  }
}

function actuallyUpdateHUD(data) {
  //console.log(data);

  // РџСЂРѕРІРµСЂСЏРµРј, РµСЃС‚СЊ Р»Рё РєРѕРјР°РЅРґР° РЅР° СЃРєСЂС‹С‚РёРµ РёРіСЂРѕРєРѕРІ
  if (data.type === "hide_players" && Array.isArray(data.steamids)) {
    // РЎРѕС…СЂР°РЅСЏРµРј steamid РІ localStorage
    localStorage.setItem("hiddenSteamId1", data.steamids[0] || "");
    localStorage.setItem("hiddenSteamId2", data.steamids[1] || "");

    // РћР±РЅРѕРІР»СЏРµРј РіР»РѕР±Р°Р»СЊРЅСѓСЋ РїРµСЂРµРјРµРЅРЅСѓСЋ РґР»СЏ С…СЂР°РЅРµРЅРёСЏ СЃРєСЂС‹С‚С‹С… Steam64 ID
    window.gsiHiddenSteamIds = data.steamids.filter((id) => id);

    console.log("РџРѕР»СѓС‡РµРЅС‹ ID РґР»СЏ СЃРєСЂС‹С‚РёСЏ С‡РµСЂРµР· GSI:", window.gsiHiddenSteamIds);
    return; // РџСЂРµСЂС‹РІР°РµРј РІС‹РїРѕР»РЅРµРЅРёРµ, С‚Р°Рє РєР°Рє СЌС‚Рѕ РєРѕРјР°РЅРґР°, Р° РЅРµ РѕР±РЅРѕРІР»РµРЅРёРµ HUD
  }

  // Р¤РёР»СЊС‚СЂСѓРµРј РёРіСЂРѕРєРѕРІ, РєРѕС‚РѕСЂС‹С… РЅСѓР¶РЅРѕ СЃРєСЂС‹С‚СЊ
  if (data.allplayers) {
    data.allplayers = filterHiddenPlayers(data.allplayers);
  }

  const slot = data.player?.slot;

  // РџСЂРѕРІРµСЂСЏРµРј РёР·РјРµРЅРµРЅРёРµ С„Р°Р·С‹ СЂР°СѓРЅРґР°
  if (
    data.round &&
    (data.round.phase === "over" || data.round.phase === "freezetime")
  ) {
    clearAllGrenades();
  }

  // РћР±РЅРѕРІР»СЏРµРј РіР»РѕР±Р°Р»СЊРЅСѓСЋ РїРµСЂРµРјРµРЅРЅСѓСЋ freezetime
  if (data.round) {
    freezetime = data.round.phase === "freezetime";
  }

  // РЎРѕС…СЂР°РЅСЏРµРј allplayers РІ РіР»РѕР±Р°Р»СЊРЅСѓСЋ РїРµСЂРµРјРµРЅРЅСѓСЋ РґР»СЏ РґРѕСЃС‚СѓРїР° РёР· РґСЂСѓРіРёС… С„СѓРЅРєС†РёР№
  window.allplayers = data.allplayers || {};

  const left = Object.values(data.allplayers || {}).filter(
    (player) => player.team === "CT"
  );
  const right = Object.values(data.allplayers || {}).filter(
    (player) => player.team === "T"
  );
  const teams_players = {
    left: left.length > 0 ? { side: "ct", players: left } : null,
    right: right.length > 0 ? { side: "t", players: right } : null,
  };

  const phase = data.phase_countdowns;
  const round = data.round;
  const map = data.map;
  const previously = data.previously;
  const bomb = data.bomb;
  const match = data.match;
  // РСЃРїРѕР»СЊР·СѓРµРј Р»РѕРєР°Р»СЊРЅСѓСЋ РїРµСЂРµРјРµРЅРЅСѓСЋ РґР»СЏ round_now, С‡С‚РѕР±С‹ РЅРµ РєРѕРЅС„Р»РёРєС‚РѕРІР°С‚СЊ СЃ РіР»РѕР±Р°Р»СЊРЅРѕР№
  const current_round = data.round_now;

  const teams = {
    left:
      left[0]?.team === "CT"
        ? {
            side: "ct",
          }
        : {
            side: "t",
          },
    right:
      right[0]?.team === "CT"
        ? {
            side: "ct",
          }
        : {
            side: "t",
          },
  };

  // РћР±РЅРѕРІР»СЏРµРј HUD РєРѕРјРїРѕРЅРµРЅС‚С‹
  updateTopPanel(
    left,
    right,
    data.map,
    data.phase_countdowns,
    data.bomb,
    data.match,
    current_round
  );
  updateRoundNow(data.round, data.map);
  updateRoundState(
    data.phase_countdowns,
    data.round,
    data.map,
    data.previously,
    data.bomb,
    data.allplayers, // РџРµСЂРµРґР°РµРј allplayers РІРјРµСЃС‚Рѕ players РґР»СЏ РїСЂР°РІРёР»СЊРЅРѕРіРѕ РѕРїСЂРµРґРµР»РµРЅРёСЏ РёРіСЂРѕРєР°, СЂР°Р·РјРёРЅРёСЂСѓСЋС‰РµРіРѕ Р±РѕРјР±Сѓ
    left,
    right,
    teams
  );
  fillObserved(data.player);
  updatePlayers(
    data.allplayers,
    data.players,
    data.observed,
    data.phase,
    data.previously,
    teams,
    teams_players,
    data.map
  );
  updateTeamValues(left, right, data.map);
  countNades(left, right);
  updatePlayerNames(data.allplayers);
  updateRadar(
    data.allplayers,
    data.map,
    data,
    data.player,
    data.bomb,
    data.player,
    data.grenades
  );
  last_round = current_round;
}

function updateActivePlayer(allplayers, player) {
  // РџСЂРѕРІРµСЂСЏРµРј, С‡С‚Рѕ player СЃСѓС‰РµСЃС‚РІСѓРµС‚
  if (!player) return;

  // РџРѕР»СѓС‡Р°РµРј СЃР»РѕС‚ Р°РєС‚РёРІРЅРѕРіРѕ РёРіСЂРѕРєР°
  const activePlayerSlot = player.slot;

  // РџСЂРѕС…РѕРґРёРј РїРѕ РІСЃРµРј РёРіСЂРѕРєР°Рј Рё СЃСЂР°РІРЅРёРІР°РµРј observer_slot
  for (let steamid in allplayers) {
    const currentPlayer = allplayers[steamid];
    const side = currentPlayer.team.toLowerCase() === "ct" ? "left" : "right";
    let playerNumber;

    // РћРїСЂРµРґРµР»СЏРµРј РЅРѕРјРµСЂ СЃР»РѕС‚Р° С‚Р°Рє Р¶Рµ, РєР°Рє РІ updateTeams
    if (side === "left") {
      playerNumber = currentPlayer.observer_slot + 1;
    } else {
      if (currentPlayer.observer_slot === 10) {
        playerNumber = 6; // T: 10->6 (РѕС‚РѕР±СЂР°Р¶Р°РµС‚СЃСЏ РєР°Рє 11)
      } else if (currentPlayer.observer_slot === 11) {
        playerNumber = 7; // T: 11->7 (РѕС‚РѕР±СЂР°Р¶Р°РµС‚СЃСЏ РєР°Рє 12)
      } else {
        playerNumber = currentPlayer.observer_slot - 4; // T: 5->1, 6->2, ..., 9->5
      }
    }

    // РџРѕР»СѓС‡Р°РµРј РєРѕРЅС‚РµР№РЅРµСЂ РёРіСЂРѕРєР°
    const playerContainer = document.querySelector(
      `#team_${side} .player_${playerNumber}`
    );
    if (playerContainer) {
      // Р•СЃР»Рё observer_slot СЃРѕРІРїР°РґР°РµС‚ СЃ Р°РєС‚РёРІРЅС‹Рј СЃР»РѕС‚РѕРј, РґРѕР±Р°РІР»СЏРµРј РєР»Р°СЃСЃ
      if (currentPlayer.observer_slot === activePlayerSlot) {
        playerContainer.classList.add("active_spectated_player");
      } else {
        playerContainer.classList.remove("active_spectated_player");
      }
    }
  }
}

