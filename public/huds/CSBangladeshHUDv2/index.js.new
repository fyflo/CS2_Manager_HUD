function filterHiddenPlayers(allplayers) {
  if (!allplayers) return {};

  // РџРѕР»СѓС‡Р°РµРј СЃРєСЂС‹РІР°РµРјС‹Рµ Steam64 ID РёР· localStorage
  const hiddenSteamId1 = localStorage.getItem("hiddenSteamId1") || "";
  const hiddenSteamId2 = localStorage.getItem("hiddenSteamId2") || "";

  // РЎРѕР·РґР°РµРј РјР°СЃСЃРёРІ ID РґР»СЏ СЃРєСЂС‹С‚РёСЏ
  const hideIds = [hiddenSteamId1, hiddenSteamId2].filter((id) => id);

  // РџСЂРѕРІРµСЂСЏРµРј РЅР°Р»РёС‡РёРµ GSI РґР°РЅРЅС‹С… РґР»СЏ СЃРєСЂС‹С‚РёСЏ РёРіСЂРѕРєРѕРІ
  if (window.gsiHiddenSteamIds && window.gsiHiddenSteamIds.length > 0) {
    // Р”РѕР±Р°РІР»СЏРµРј GSI ID РІ РјР°СЃСЃРёРІ РґР»СЏ СЃРєСЂС‹С‚РёСЏ
    window.gsiHiddenSteamIds.forEach((id) => {
      if (id && !hideIds.includes(id)) {
        hideIds.push(id);
      }
    });
  }

  if (hideIds.length === 0) return allplayers;

  //console.log("РЎРєСЂС‹РІР°РµРј РёРіСЂРѕРєРѕРІ СЃ ID:", hideIds);

  // РЎРѕР·РґР°РµРј РєРѕРїРёСЋ РѕР±СЉРµРєС‚Р° allplayers Р±РµР· СЃРєСЂС‹С‚С‹С… РёРіСЂРѕРєРѕРІ
  const filteredPlayers = {};
  Object.keys(allplayers).forEach((steamid) => {
    if (!hideIds.includes(steamid)) {
      filteredPlayers[steamid] = allplayers[steamid];
    }
  });

  //console.log(
    //`РћС‚РѕР±СЂР°Р¶Р°РµРј ${Object.keys(filteredPlayers).length} РёР· ${
    //  Object.keys(allplayers).length
    //} РёРіСЂРѕРєРѕРІ`
  //);
  return filteredPlayers;
}

